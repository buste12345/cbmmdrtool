extends ../layouts/default

block content
	.container: .row: .col-sm-10.col-sm-offset-1.col-md-8.col-md-offset-2
		article
			h1 MRs submissions
			if !data.mmdrs
				h3.text-muted Error. No results.
			else
				table
					thead
					tbody
					for mmdr in data.mmdrs
						tr
							td= mmdr
							td
								a(href='/mgui') Crawl